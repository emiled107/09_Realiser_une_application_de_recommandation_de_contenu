{
    "bindings": [
      {
        "authLevel": "anonymous",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "methods": ["get", "post"],
        "dataType": "binary"
      },
      {
        "type": "http",
        "direction": "out",
        "name": "$return"
      }
    ],
    "scriptFile": "function.py",
    "entryPoint": "main"
  }